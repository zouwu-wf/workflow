{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist/**",
        "{projectRoot}/out/**"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "{projectRoot}/src/**/*.tsx",
        "{projectRoot}/src/**/*.ts",
        "{projectRoot}/test/**/*.ts"
      ],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ],
      "cache": true
    },
    "dev": {
      "cache": false
    }
  }
}